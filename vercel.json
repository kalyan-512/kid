{
  "buildCommand": null,
  "outputDirectory": "kid",
  "cleanUrls": true,
  "trailingSlash": false,
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|gif|svg|webp|css)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/programs", "destination": "/programs.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/preschools", "destination": "/preschools.html" },
    { "source": "/admission", "destination": "/admission.html" },
    { "source": "/franchise", "destination": "/franchise.html" },
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/gachibowli", "destination": "/preschool-gachibowli.html" },
    { "source": "/nallagandla", "destination": "/preschool-nallagandla.html" },
    { "source": "/attapur", "destination": "/preschool-attapur.html" },
    { "source": "/begumpet", "destination": "/preschool-begumpet.html" },
    { "source": "/ameerpet", "destination": "/preschool-ameerpet.html" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/terms-conditions", "destination": "/terms-conditions.html" }
  ]
}